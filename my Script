--Jamp

-- Сохранение настроек
local Settings = {
    JumpHeight = 50,
    AutoApply = true
}

-- Функция применения высоты прыжка
function ApplyJumpHeight()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.JumpPower = Settings.JumpHeight
            print("Высота прыжка установлена: " .. Settings.JumpHeight)
        end
    end
end

-- Ползунок для высоты прыжка
local JumpSlider = MainSection:NewSlider(
    "jump height", 
    "jump height edit", 
    500, 
    50, 
    function(value)
        Settings.JumpHeight = value
        if Settings.AutoApply then
            ApplyJumpHeight()
        end
    end
)

-- Автоматическое применение при появлении персонажа
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    character:WaitForChild("Humanoid")
    wait(0.5)
    if Settings.AutoApply then
        ApplyJumpHeight()
    end
end)

-- Применяем настройки при запуске
if game.Players.LocalPlayer.Character then
    ApplyJumpHeight()
end
